<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/logorespiranetresumida"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logorespiranetresumida"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetoRespiraNet">
        <meta-data
            android:name="org.osmdroid.CONFIGURATION_FILE"
            android:value="osmdroid/configuration.xml"/>
        <activity android:name=".MapsActivity" android:launchMode="singleTop"/>
        <activity android:name=".GraphicsActivity" android:launchMode="singleTop"/>
<!--        <activity android:name=".SettingsActivity"/>-->
        <activity
            android:name=".MainActivity"
            android:exported="true" android:launchMode="singleTop">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
</manifest>